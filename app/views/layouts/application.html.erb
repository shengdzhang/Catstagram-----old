<!DOCTYPE html>
<html>
<head>
  <title>Finalproject</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<header>
  <div class="head">
    <% if logged_in? %>
      <%= current_user.username %>
      <br/>
      <%= button_to "Log Out", session_url, method: :delete%>
    <% end %>
  </div>
</header>

<%= render "layouts/errors" %>

<%= yield %>


</body>
</html>
