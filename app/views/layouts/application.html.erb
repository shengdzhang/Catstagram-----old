<!DOCTYPE html>
<html>
<head>
  <title>Finalproject</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<header>
<% if logged_in? %>
  Welcome <%= current_user.username %> !
  <br/>
  <%= button_to "Log Out", session_url, method: :delete%>
<% end %>
</header>

<%= render "layouts/errors" %>

<%= yield %>


</body>
</html>
